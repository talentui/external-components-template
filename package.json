        "@types/react-redux": "^5.0.8",
        "@types/react-router-dom": "^4.0.7",
        "@types/react-test-renderer": "^15.5.4",
        "@types/redux-actions": "^2.2.0",
        "@types/uuid": "^3.4.2",
        "enzyme": "^2.9.1",
        "eslint": "^4.6.1",
        "jest": "^21.1.0",
        "react-test-renderer": "^15.6.1",
        "tslint": "^5.7.0",
        "typescript": "^2.5.2",
        "webpack": "^3.5.6",
        "webpack-dev-server": "^2.7.1"
    }
