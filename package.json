{
    "name": "@talent/tita-component",
    "version": "{{ version }}",
    "description": "{{ description }}",
    "main": "index.js",
    "author": "{{ author }}",
    "scripts": {
        "start": "friendly=on  dev_server=on webpack-dev-server",
        "build": "rm -rf dist/ && check=on webpack --config ./webpack.library.config.js",
        "build-prod": "rm -rf dist/ && NODE_ENV=production webpack --config ./webpack.library.config.js",
        "analysis": "analysis=on npm run build",
        "debug": "node --inspect --debug-brk node_modules/.bin/webpack",
        "debug-dev": "DEV_SERVER=on node --inspect --debug-brk node_modules/.bin/webpack-dev-server",
        "test": "jest"
    },
    "repository": {
        "type": "{{ repository-type }}",
        "url": "{{ repository-url }}"
    },
    "license": "MIT",
    "dependencies": {
        "@beisen/grid-page-builder": "^3.0.6",
        "@beisen/talent-ui-bootstrap": "^1.0.15",
        "@beisen/talent-ui-helper": "^1.0.6",
        "@talentui/bootstrap-react": "^1.0.0",
        "@talentui/component-webpack-config": "^1.0.0",
        "@talentui/dll-ethos": "^1.0.4",
        "@talentui/dll-parser": "^1.0.1",
        "@talentui/dll-react": "^1.0.0",
        "@talentui/dll-webpack-config": "^1.0.1",
        "@talentui/webpack-config": "^1.0.0",
        "@talentui/webpack-library-config": "^1.0.6",
        "react": "^15.6.2",
        "react-transition-group": "^2.2.1",
        "uuid": "^3.1.0"
